<?xml version="1.0" encoding="UTF-8"?>

 

<?import javafx.scene.control.Button?>

<?import javafx.scene.control.ComboBox?>

<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.ColumnConstraints?>

<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>





<VBox fx:controller="application.AddFoodController" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141"
	xmlns:fx="http://javafx.com/fxml/1">

	<children>

		<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="97.0">

			<children>

				<Label text="Food Addtiion">

					<font>

						<Font size="36.0" />

					</font>

				</Label>

				<GridPane prefHeight="274.0" prefWidth="500.0">

					<columnConstraints>

						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
							prefWidth="100.0" />

						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
							prefWidth="100.0" />

					</columnConstraints>

					<rowConstraints>

						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />

						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />

						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />

						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />

						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />

					</rowConstraints>

					<children>

						<Label text="Food : " GridPane.halignment="RIGHT">

							<font>

								<Font size="22.0" />

							</font>

						</Label>

						<Label text="kcal : " GridPane.halignment="RIGHT"
							GridPane.rowIndex="1">

							<font>

								<Font size="22.0" />

							</font>

						</Label>

						<Label text="Carbohydrate : " GridPane.halignment="RIGHT"
							GridPane.rowIndex="2">

							<font>

								<Font size="22.0" />

							</font>

						</Label>

						<Label text="Protein : " GridPane.halignment="RIGHT"
							GridPane.rowIndex="3">

							<font>

								<Font size="22.0" />

							</font>

						</Label>

						<Label text="Fat : " GridPane.halignment="RIGHT"
							GridPane.rowIndex="4">

							<font>

								<Font size="22.0" />

							</font>

						</Label>

						<ComboBox fx:id="comboBox" onAction="#getFood"
							prefHeight="30.0" prefWidth="194.0" GridPane.columnIndex="1" />

						<Label fx:id="kcal" text="Label" GridPane.columnIndex="1"
							GridPane.rowIndex="1">

							<font>

								<Font size="18.0" />

							</font>

						</Label>

						<Label fx:id="carbo" text="Label" GridPane.columnIndex="1"
							GridPane.rowIndex="3">

							<font>

								<Font size="18.0" />

							</font>

						</Label>

						<Label fx:id="fat" text="Label" GridPane.columnIndex="1"
							GridPane.rowIndex="2">

							<font>

								<Font size="18.0" />

							</font>

						</Label>

						<Label fx:id="protein" text="Label" GridPane.columnIndex="1"
							GridPane.rowIndex="4">

							<font>

								<Font size="18.0" />

							</font>

						</Label>

					</children>

				</GridPane>
				<Button mnemonicParsing="false" prefHeight="68.0" prefWidth="107.0"
					text="Add" onAction="#addFood">

					<font>

						<Font size="22.0" />

					</font>

				</Button>

			</children>


		</VBox>

	</children>

</VBox>
